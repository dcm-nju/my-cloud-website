{
    "version": 2,
    "builds": [
        {
            "src": "wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "wsgi.py"
        }
    ],
    "functions": {
        "wsgi.py": {
            "maxDuration": 10,
            "runtime": "python3.9"
        }
    },
    "env": {
        "FLASK_ENV": "production"
    }
}